obj corpse;
foreach (corpse, obj_in_room($n->in_room)) {
        if (corpse->item_type != ITEM_CORPSE_NPC)
                continue;
 
        act("$n savagely devours a corpse.", $n, NULL, NULL, TO_ROOM);
        extract_obj(corpse, XO_F_NORECURSE);
        return 1;
}
 
return 0;
