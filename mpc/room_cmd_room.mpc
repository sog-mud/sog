act("Players in this room:", $n, NULL, NULL, TO_CHAR);
char victim;
foreach (victim, char_in_room($n->in_room)) {
        if (is_npc(victim))
                act("  [{cnpc{x]$N", $n, NULL, victim, TO_CHAR);
        else
                act("  [{c pc{x]$N", $n, NULL, victim, TO_CHAR);
}
return 0;
