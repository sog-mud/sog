if (number_bits(2) != 0)
        return 0;

char victim = get_random_fighting($n);
if (victim == NULL)
        return 0;

if (char_level($n) > 36)
	cast_char("lightning breath", $n, victim);
else
	cast_char("lightning balt", $n, victim);

return 0;
