act("{y$n dissapears. Only peace of wooden bark remains on the ground.{x",
    $n, NULL, NULL, TO_ROOM);
 
obj wear = get_eq_char($n, WEAR_BODY);
if (wear != NULL
&&  obj_vnum(wear) == 35482)
        return 0;

obj bark = create_obj(35465, 0);
obj_to_room(bark, char_room($n));
 
return 0;
