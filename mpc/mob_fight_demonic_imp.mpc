char victim = get_random_fighting($n);
if (victim == NULL)
       return 0;

act("{DA $n's shriek resounds through the cave!{x",
    $n, NULL, NULL, TO_ROOM);
damage($n, victim, number_range(50, 80), NULL, DAM_SOUND, DAM_F_NONE);
return 0;
