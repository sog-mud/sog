if (get_obj_carry($n, $N, "21019"))
        return 0;

look_char($n, $N);
tell_char($n, $N, "You don't look too well!");
 
obj o;
if ((o = get_obj_carry($n, $n, "21019")) == NULL)
        o = load_obj($n, 21019);

give_obj($n, $N, o);
