if (is_owner($p, $n)) {
        act("Your $p starts glowing.", $n, $p, NULL, TO_CHAR);
        return 0;
}
 
act("You are zapped by $p and drop it.", $n, $p, NULL, TO_CHAR);
act("$n is zapped by $p and drops it.", $n, $p, NULL, TO_ROOM);
obj_to_room($p, $n->in_room);
return 0;
