int type = obj_item_type($p);
 
if (type == ITEM_GEM) {
        dofun("say", $n, "Nice gem...");
        dofun("say", $n, "I'll take it, but it's not enough payment for the oak sapling.");
        extract_obj($p, 0);
        return 0;
}
 
dofun("say", $n, "I don't need it. Take it back.");
give_obj($n, $N, $p);
return 0;
