 */

int index; int sector; int times;

times = 1;
sector = room_sector(char_room($n));

if (sector != SECT_WATER_SWIM && sector != SECT_WATER_NOSWIM)
        return 0;

act("You skip $p by the ocean...", $n, $p, NULL, TO_CHAR);
act("$n skipped $p by the ocean...", $n, $p, NULL, TO_ROOM);
foreach (index, range(1, real_char_level($n))) {
        if (number_percent() < char_level($n))
                times++;
