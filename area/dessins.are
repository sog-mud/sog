if (times > 1)
        act("Wow! $p skipped $K $qK{times}", $n, $p, times, TO_ALL);
else
        act("Дурацкий булыжник! Даже прыгать не умеет!", $n, $p, NULL, TO_CHAR);
extract_obj($p, 0);
return 0;
