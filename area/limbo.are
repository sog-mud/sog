       act("The $p shivers uncontrollably!", actor, obj, NULL, TO_ROOM);
       act("Your $p shivers uncontrollably!", actor, obj, NULL, TO_CHAR);
       spellfun("confuse", NULL, actor->level, actor, actor->fighting);
       wait_state(actor, 2 * get_pulse("violence"));
       break;
}
return 0;
