m obj_get @ 100~
if (is_owner(obj, actor)) {
        if (is_affected(actor , AFF_POISON)
        &&  dice(1, 5) == 1) {
                 act("Your $p glows blue.", actor, obj, NULL, TO_CHAR);
                 spellfun("cure poison", NULL, 30, actor, actor);
                 return 0;
        }
        if (is_affected(actor , AFF_CURSE)
        &&  dice(1, 5) == 1) {
                 act("Your $p glows blue.", actor, obj, NULL, TO_CHAR);
                 spellfun("remove curse", NULL, 30, actor, actor);
                 return 0;
        }
        act("Your $p's humming gets lauder.", actor, obj, NULL, TO_CHAR);
        return 0;
}
 
act("You are zapped by $p and drop it.", actor, obj, NULL, TO_CHAR);
obj_to_room(obj, actor->in_room);
 
switch (dice(1, 10)) {
case 1:
       spellfun("curse", NULL, actor->level, actor, actor);
       break;
case 2:
       spellfun("poison", NULL, actor->level, actor, actor);
       break;
}
return 0;
