       act("Your $p trembles violently!", actor, obj, NULL, TO_CHAR);
       spellfun("mirror", NULL, actor->level, actor, actor);
       wait_state(actor, 2 * get_pulse("violence"));
       break;
case 1:
       act("The $p shakes a bit.", actor, obj, NULL, TO_ROOM);
       act("Your $p shakes a bit.", actor, obj, NULL, TO_CHAR);
       spellfun("garble", NULL, actor->level, actor, actor->fighting);
       wait_state(actor, 2 * get_pulse("violence"));
       break;
