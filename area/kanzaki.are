        dofun("say", mob, "Greetings!");
        social_char("bow", mob, actor);
} else if (is_neutral(actor)) {
        mob_interpret(mob, "yawn");
        dofun("say", mob, "What have brought you here?");
} else if (is_evil(actor)) {
        dofun("say", mob, "You must have an awesome reason to come here.");
        social_char("grin", mob, actor);
}
return 0;
~
m mob_random mob_random_guard 100~
m mob_speech @ i'm your woman~
if (is_good(actor)) {
        dofun("say", mob, "You may be invited by my master, but better wait for him.");
        social_char("smile", mob, actor);
} else if (is_neutral(actor))
        dofun("say", mob, "Do not test my tolerance. Just wait.");
else if (is_evil(actor)) {
        dofun("say", mob, "Who are you trying to fool?");

        Room r = get_room_index(47002);
        if (r == NULL)
                return 0;

        social_char("wave", mob, actor);
        transfer_char(actor, r);
}
return 0;
~
m mob_speech @ password~
dofun("say", mob, "Heh heh, what a kid!");
if (!is_sn_affected(mob, "frenzy"))
        cast_char("frenzy", mob, mob);
 
Room r = get_room_index(10495);
if (r == NULL)
        return 0;
 
dofun("say", mob, "Have a nice day in hell today.");
transfer_char(actor, r);
return 0;
~
m mob_speech mob_speech_lothar_warrior_password пароль~
m mob_speech mob_speech_lothar_warrior i'm your man~
m mob_exit @ +west~
dofun("emote", mob, "comes to life!");
act("$n steps forth, forbidding you to enter.", mob, NULL, actor, TO_VICT);
act("$n steps forth, blocking $N passage.", mob, NULL, actor, TO_NOTVICT);
dofun("say", mob, "You aren't allowed there, unless you know the password.");
return 1;
~
m mob_cmd @ shoes~
act("You remove your shoes.", mob, NULL, actor, TO_VICT);
act("$N removes $s shoes.", mob, NULL, actor, TO_NOTVICT);
 
Room r = get_room_index(47000);
if (r == NULL)
        return 0;

act_say(mob, "Welcome, $N!", actor);
 
act("The statue escorts you to the master's chamber.",
    mob, NULL, actor, TO_VICT);
act("The statue opens the west door, letting $N enter.",
    mob, NULL, actor, TO_NOTVICT);
transfer_char(actor, r);
return 0;
~
